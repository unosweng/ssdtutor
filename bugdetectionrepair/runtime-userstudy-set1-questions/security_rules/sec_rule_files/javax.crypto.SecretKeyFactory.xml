<?xml version="1.0" encoding="UTF-8"?>
<rules id="r-javax-secretkeyfactory" class="javax.crypto.SecretKeyFactory">
    <rule method="getInstance">
        <argument index="1" name="transformation" searchType="equals">
            <value string="AES" security="strong"/>
            <value string="DES" security="weak"/>
            <value string="DESede" security="weak"/>
            <value string="ARCFOUR" security="weak"/>
            <value string="PBEWithMD5AndDES" security="strong"/>
            <value string="\bPBE.*\b" security="strong"/>
            <value string="PBKDF2WithHmacSHA1" security="strong"/>
            <value string="\bPBKDF.*\b" security="strong"/>
            <value string="\bAES.*\b" security="weak"/>
        </argument>
        <argument index="2" name="provider">
            <value string="(.*?)" />
            <value reference="r-javax-provider" />
        </argument>	
    </rule>
    <rule method="generateSecret">
        <argument index="1" name="keySpec">
            <value keySpec="(.*?)"/>
        </argument>	
    </rule>
    <rule method="getKeySpec">
        <argument index="1" name="secretKey">
            <value key="(.*?)"/>
        </argument>
        <argument index="1" name="class">
            <value keySpec="(.*?)"/>
        </argument>	
    </rule>
    <rule method="translateKey">
        <argument index="1" name="secretKey">
            <value key="(.*?)"/>
        </argument>
    </rule>
</rules>
